@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix focu: <http://focu.io/schema#> .
@prefix focudata: <http://focu.io/data#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix core: <http://vivoweb.org/ontology/core#> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix npg: <http://ns.nature.com/terms/> .
@prefix teach: <http://linkedscience.org/teach/ns#>.


focu:University
  a rdfs:Class ;
  rdfs:subClassOf foaf:Organization ;
  rdfs:label "University"@en ;
  rdfs:comment "University Class"@en.

# the follow vocabulary can use
npg:number
    a owl:DatatypeProperty ;
    rdfs:domain npg:Component ;
    rdfs:isDefinedBy <http://ns.nature.com/terms/> ;
    rdfs:range xsd:integer ;
    skos:definition "The :number property specifies a sequence number for a component such as a figure."@en ;
    skos:prefLabel "Number"@en .

npg:name
    a owl:DatatypeProperty ;
    rdfs:domain npg:Contributor ;
    rdfs:isDefinedBy <http://ns.nature.com/terms/> ;
    rdfs:range rdfs:Literal ;
    skos:definition "The :name property specifies the full name for a thing."@en ;
    skos:prefLabel "Name"@en .

npg:Subject
    a owl:Class ;
    rdfs:isDefinedBy <http://ns.nature.com/terms/> ;
    rdfs:subClassOf npg:Type ;
    skos:definition "The :Subject class represents a topic, field of study or research area which can be used to categorise the contents of a publication. The instances of :Subject are organised hierarchically using the SKOS vocabulary."@en ;
    skos:prefLabel "Subject"@en .

npg:filename
    a owl:DatatypeProperty ;
    rdfs:domain npg:Asset ;
    rdfs:isDefinedBy <http://ns.nature.com/terms/> ;
    rdfs:range rdfs:Literal ;
    skos:definition "The :filename property specifies the filename of an asset."@en ;
    skos:prefLabel "Filename"@en .

npg:example
    a owl:AnnotationProperty ;
    rdfs:isDefinedBy <http://ns.nature.com/terms/> ;
    skos:definition "The :example property specifies an example of an entity."@en ;
    skos:note "This property is similar to void:exampleResource. But see also skos:example."@en ;
    skos:prefLabel "Example"@en .

npg:id
    a owl:DatatypeProperty ;
    rdfs:isDefinedBy <http://ns.nature.com/terms/> ;
    rdfs:range rdfs:Literal ;
    skos:definition "The :id property provides a unique ID for a thing."@en ;
    skos:note "The :id value forms the local part of the full URI for an entity name."@en ;
    skos:prefLabel "ID"@en .

npg:hasItem
    a owl:ObjectProperty ;
    rdfs:domain npg:Publication ;
    rdfs:isDefinedBy <http://ns.nature.com/terms/> ;
    rdfs:range npg:Item ;
    skos:definition "The :hasItem property relates a publication to an item."@en ;
    skos:prefLabel "Has Item"@en .

npg:hasSubject
    a owl:ObjectProperty ;
    rdfs:domain npg:Publication ;
    rdfs:isDefinedBy <http://ns.nature.com/terms/> ;
    rdfs:range npg:Subject ;
    skos:definition "The :hasSubject property relates a publication to a subject term which describes one of the main topics the publication is about."@en ;
    skos:prefLabel "Has Subject"@en .

core:University a owl:Class ;
    rdfs:label "University"@en ;
    obo:IAO_0000112 "Definition taken from: http://en.wikipedia.org/wiki/University"^^xsd:string,
        "University of Florida; Washington University in St. Louis"^^xsd:string ;
    obo:IAO_0000115 "An institution of higher education and research, which grants academic degrees in a variety of subjects, and provides both undergraduate education and postgraduate education."^^xsd:string ;
    rdfs:subClassOf <http://xmlns.com/foaf/0.1/Organization> .

core:Course a owl:Class ;
    rdfs:label "Course"@en ;
    obo:IAO_0000112 "A course as taught in one time period (such as a semester; although note that a course could consist of only one meeting (teaching session)) by one or more instructors, normally but not always for credit. Does not represent either each meeting of the course or the course offering such as Biology 101 taught every semester from 1980 to 2010"^^xsd:string ;
    obo:IAO_0000115 "A course as taught in one time period by one or more instructors, normally but not always for credit. Does not represent either each meeting of the course or the course offering such as Biology 101 taught every semester from 1980 to 2010"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom rdfs:Literal ;
            owl:onProperty core:courseCredits ],
        <http://purl.org/NET/c4dm/event.owl#Event> .
  
core:Laboratory a owl:Class ;
    rdfs:label "Laboratory"@en ;
    obo:IAO_0000115 "An organization unit that facilitates or conduits observation, testing, experimentation, or research in a field of study or practice."^^xsd:string,
        "An organizational unit (as opposed to the physical facility) that performs research, provides services, or processes materials"^^xsd:string ;
    rdfs:subClassOf <http://xmlns.com/foaf/0.1/Organization> .
	
core:Dataset a owl:Class ;
    rdfs:label "Dataset"@en ;
    obo:IAO_0000112 "US Patent Data; US Job Data"^^xsd:string ;
    obo:IAO_0000115 "A named collection of data, usually containing only one type of data"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://purl.org/ontology/bibo/Document> ;
            owl:onProperty <http://purl.org/spar/cito/isCitedAsDataSourceBy> ],
        <http://purl.org/ontology/bibo/Document> .
	
core:courseCredits a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "credits"@en .
	
core:description a owl:DatatypeProperty ;
    rdfs:label "description"@en .
	
core:Association a owl:Class ;
    rdfs:label "Association"@en ;
    obo:IAO_0000112 "Special Libraries Association; Association for Computing Machinery(ACM); American Medical Informatics Association(AMIA)"^^xsd:string ;
    obo:IAO_0000115 "A formal organization of people or groups of people around a subject or practice."^^xsd:string,
        "A group of persons or organizations organized for a common purpose."^^xsd:string ;
    rdfs:subClassOf <http://xmlns.com/foaf/0.1/Organization> .

core:Department a owl:Class ;
    rdfs:label "Department"@en ;
    obo:IAO_0000112 "Definition modified from the definition here: http://dictionary.reference.com/browse/department. It is difficult to tell the difference between and department and a division."^^xsd:string,
        "Legal (department within a company); Use for any non-academic department"^^xsd:string ;
    obo:IAO_0000115 "A unit within a larger organization that addresses a specific subject or area of activity."^^xsd:string ;
    rdfs:subClassOf <http://xmlns.com/foaf/0.1/Organization> .	
	
core:Presentation a owl:Class ;
    rdfs:label "Presentation"@en ;
    obo:IAO_0000115 "Encompasses talk, speech, lecture, slide lecture, conference presentation"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/NET/c4dm/event.owl#Event> .
	
core:Project a owl:Class ;
    rdfs:label "Project"@en ;
    obo:IAO_0000112 """An endeavor, frequently collaborative, that occurs over a finite period of time and is intended to achieve a particular aim.
"""^^xsd:string ;
    obo:IAO_0000115 "An endeavor, frequently collaborative, that occurs over a finite period of time and is intended to achieve a particular aim."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom core:DateTimeInterval ;
            owl:onProperty core:dateTimeInterval ],
        [ a owl:Restriction ;
            owl:allValuesFrom rdfs:Literal ;
            owl:onProperty core:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom core:GeographicRegion ;
            owl:onProperty core:geographicFocus ],
        obo:BFO_0000015 .
	
core:subjectAreaOf a owl:ObjectProperty ;
    rdfs:label "subject area of"@en ;
    rdfs:subPropertyOf core:conceptAssociatedWith .
	
core:hasSubjectArea a owl:ObjectProperty ;
    rdfs:label "has subject area"@en ;
    rdfs:subPropertyOf core:hasAssociatedConcept ;
    owl:inverseOf core:subjectAreaOf .
	

core:abbreviation a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "abbreviation"@en ;
    obo:IAO_0000112 "A short form for an longer title or name."^^xsd:string,
        "B.A."^^xsd:string .
		
core:identifier a owl:DatatypeProperty ;
    rdfs:label "identifier"@en .
	
skos:related a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "related concept"@en ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept .
	
core:Video a owl:Class ;
    rdfs:label "Video"@en ;
    obo:IAO_0000115 "Audiovisual recording in video format"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/ontology/bibo/AudioVisualDocument> .

core:departmentOrSchool a owl:DatatypeProperty ;
    rdfs:label "department or school name within institution"@en ;
    vitro:descriptionAnnot "Not intended to be an institution name."^^xsd:string ;
    rdfs:domain core:EducationalProcess .

core:hasPrerequisite a owl:ObjectProperty ;
    rdfs:label "has prerequisite"@en ;
    rdfs:domain core:Course ;
    rdfs:range core:Course ;
    owl:inverseOf core:prerequisiteFor .
	
core:informationResourceSupportedBy a owl:ObjectProperty ;
    rdfs:label "supported by"@en ;
    vitro:descriptionAnnot "an information resource (typically a publication) supported by (typically via funding) an agreement (such as a grant)"^^xsd:string ;
    rdfs:domain obo:IAO_0000030 ;
    rdfs:subPropertyOf core:supportedBy ;
    owl:inverseOf core:supportedInformationResource .

core:majorField a owl:DatatypeProperty ;
    rdfs:label "major field of degree"@en ;
    obo:IAO_0000112 "Information Science; Computer Science; Anthropology"^^xsd:string ;
    rdfs:domain core:EducationalProcess .

core:offers a owl:ObjectProperty ;
    rdfs:label "offers"@en .
	
core:offeredBy a owl:ObjectProperty ;
    rdfs:label "offered by"@en ;
    owl:inverseOf core:offers .
	
core:overview a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "overview"@en ;
    obo:IAO_0000112 "My research focuses on diseases of pine crops in the southeastern United States."@en ;
    obo:IAO_0000115 "Short text for presentation describing the agent's purpose, activities, and/or accomplishments."@en ;
    rdfs:domain <http://xmlns.com/foaf/0.1/Agent> .
	
core:seatingCapacity a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "seating capacity"@en ;
    obo:IAO_0000112 "55"^^xsd:string,
        "definition modified from: source (http://en.wikipedia.org/wiki/Seating_capacity)."^^xsd:string ;
    rdfs:domain core:Room ;
    rdfs:range xsd:int .

core:start a owl:ObjectProperty ;
    rdfs:label "start"@en ;
    rdfs:domain core:DateTimeInterval ;
    rdfs:range core:DateTimeValue ;
    rdfs:subPropertyOf core:dateTimeValue .

core:termLabel a owl:DatatypeProperty ;
    rdfs:label "term label"@en .

core:termType a owl:DatatypeProperty ;
    rdfs:label "term type"@en .
	
core:teachingOverview a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "teaching overview"@en ;
    obo:IAO_0000112 "Used for a single narrative summary of teaching, typically covering a wide range of courses including for credit and non-credit teaching over multiple semesters; the \"teaches\" property links a person directly with an instance of a Semester Class, typically from an institutional database of record; then use Teacher Role for information about a person's role in non-credit teaching or their specific contribution to individual courses"^^xsd:string ;
    rdfs:domain <http://xmlns.com/foaf/0.1/Person> .
	
core:rank a owl:DatatypeProperty ;
    rdfs:label "rank"@en ;
    obo:IAO_0000112 "this number indicates a position in a list"^^xsd:string ;
    rdfs:range xsd:int .
	
core:supplementalInformation a owl:DatatypeProperty ;
    rdfs:label "supplemental information"@en .
	
vcard:hasURL a owl:ObjectProperty ;
    rdfs:label "has url"@en ;
    rdfs:comment "To specify a uniform resource locator associated with the object"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
    owl:equivalentProperty :url 
	
vcard:hasSource a owl:ObjectProperty ;
    rdfs:label "has source"@en ;
    rdfs:comment "To identify the source of directory information of the object"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> .
	
vcard:hasCategory a owl:ObjectProperty ;
    rdfs:label "has category"@en ;
    rdfs:comment "Used to support property parameters for the category data property"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> .
	
vcard:hasAdditionalName a owl:ObjectProperty ;
    rdfs:label "has additional name"@en ;
    rdfs:comment "Used to support property parameters for the additional name data property"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> .

vcard:hasFN a owl:ObjectProperty ;
    rdfs:label "has formatted name"@en ;
    rdfs:comment "Used to support property parameters for the formatted name data property"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> .
	
vcard:hasRelated a owl:ObjectProperty ;
    rdfs:label "has related"@en ;
    rdfs:comment "To specify a relationship between another entity and the entity represented by this object"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> .
	
vcard:hasTitle a owl:ObjectProperty ;
    rdfs:label "has title"@en ;
    rdfs:comment "Used to support property parameters for the title data property"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> .

vcard:hasUID a owl:ObjectProperty ;
    rdfs:label "has uid"@en ;
    rdfs:comment "To specify a value that represents a globally unique identifier corresponding to the object"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> .
	
vcard:Name a owl:Class ;
    rdfs:label "Name"@en ;
    rdfs:comment "To specify the components of the name of the object"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( [ a owl:Class ;
                        owl:intersectionOf ( [ a owl:Restriction ;
                                    owl:onProperty :additional-name ;
                                    owl:someValuesFrom xsd:string ] [ a owl:Restriction ;
                                    owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                                    owl:onProperty :additional-name ] ) ] [ a owl:Class ;
                        owl:intersectionOf ( [ a owl:Restriction ;
                                    owl:onProperty :family-name ;
                                    owl:someValuesFrom xsd:string ] [ a owl:Restriction ;
                                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onProperty :family-name ] ) ] [ a owl:Class ;
                        owl:intersectionOf ( [ a owl:Restriction ;
                                    owl:onProperty :given-name ;
                                    owl:someValuesFrom xsd:string ] [ a owl:Restriction ;
                                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onProperty :given-name ] ) ] [ a owl:Class ;
                        owl:intersectionOf ( [ a owl:Restriction ;
                                    owl:onProperty :honorific-prefix ;
                                    owl:someValuesFrom xsd:string ] [ a owl:Restriction ;
                                    owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                                    owl:onProperty :honorific-prefix ] ) ] [ a owl:Class ;
                        owl:intersectionOf ( [ a owl:Restriction ;
                                    owl:onProperty :honorific-suffix ;
                                    owl:someValuesFrom xsd:string ] [ a owl:Restriction ;
                                    owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                                    owl:onProperty :honorific-suffix ] ) ] ) ] .

vcard:VCard a owl:Class ;
    rdfs:label "VCard"@en ;
    rdfs:comment "The vCard class is  equivalent to the new Kind class, which is the parent for the four explicit types of vCards (Individual, Organization, Location, Group)"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
    owl:equivalentClass :Kind .

vcard:fn a owl:DatatypeProperty ;
    rdfs:label "formatted name"@en ;
    rdfs:comment "The formatted text corresponding to the name of the object"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
    rdfs:range xsd:string .
	
vcard:url a owl:ObjectProperty ;
    rdfs:label "url"@en ;
    rdfs:comment "This object property has been mapped"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
    owl:equivalentProperty :hasURL .
	
vcard:n a owl:ObjectProperty ;
    rdfs:label "name"@en ;
    rdfs:comment "This object property has been mapped"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
    owl:equivalentProperty :hasName .
	
vcard:hasName a owl:ObjectProperty ;
    rdfs:label "has name"@en ;
    rdfs:comment "To specify the components of the name of the object"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
    rdfs:range :Name ;
	
vcard:organization-unit a owl:DatatypeProperty ;
    rdfs:label "organizational unit name"@en ;
    rdfs:comment "To specify the organizational unit name associated with the object"@en ;
    rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf :organization-name .
	
